; Top-most http://editorconfig.org/ file
root = true

[*]
end_of_line = lf
charset = utf-8
indent_style = tab
indent_size = 4
insert_final_newline = true

# Config
[*.json]
indent_style = space
indent_size = 2

# WEB
[*.{htm,html}]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 2

# Markdown
[*.md]
indent_style = space
indent_size = 4

# Markdown
[json.2]
indent_style = space
indent_size = 2

